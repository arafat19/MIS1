<div id="application_top_panel" class="panel panel-primary">

    <div class="panel-heading">
        <div class="panel-title">
            Create User Account
        </div>
    </div>

    <form id='searchForm' name='searchForm' class="form-horizontal form-widgets" role="form">
        <div class="panel-body">
            <div class="form-group">
                <label class="col-md-2 control-label label-required" for="customerCode">Customer A/C No:</label>
                <div class="col-md-3">
                    <input type="text" class='required digits k-textbox' id="customerCode" name="customerCode"
                           value="" tabindex="1"/>
                </div>
            </div>
        </div>

        <div class="panel-footer">
            <button name="showReport" id="showReport" type="submit" data-role="button"
                    class="k-button k-button-icontext"
                    role="button" tabindex="2"
                    aria-disabled="false"><span class="k-icon k-i-search"></span>Search
            </button>
        </div>
    </form>
</div>

