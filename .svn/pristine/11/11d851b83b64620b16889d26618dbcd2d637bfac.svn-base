<div id="application_top_panel" class="panel panel-primary">
    <div class="panel-heading">

        <div class="panel-title">
            Search Open Backlog
        </div>

    </div>
    <form id="openBacklogForm" name="openBacklogForm" class="form-horizontal form-widgets" role="form">
        <div class="panel-body">
            <div class="form-group">
                <label class="col-md-1 control-label label-required" for="projectId">Project:</label>

                <div class="col-md-3">
                    <pt:dropDownProject
                            tabindex="1"
                            onchange="onChangeProject();"
                            dataModelName="dropDownProject"
                            name="projectId">
                    </pt:dropDownProject>
                </div>

                <div class="col-md-2 pull-left">
                    <span class="k-invalid-msg" data-for="projectId"></span>
                </div>



                <label class="col-md-1 control-label label-optional" for="moduleId">Module:</label>

                <div class="col-md-3">
                    <select tabindex="2"
                            name="moduleId"
                            id="moduleId"
                            hintText="ALL">
                    </select>
                </div>

                <div class="col-md-2 pull-left">
                    <span class="k-invalid-msg" data-for="moduleId"></span>
                </div>
            </div>
        </div>

        <div class="panel-footer">

            <button id="search" name="search" type="submit" data-role="button" class="k-button k-button-icontext"
                    role="button" tabindex="3"
                    aria-disabled="false"><span class="k-icon k-i-search"></span>Search
            </button>
            <span class="download_icon_set">
                <ul>
                    <li>Save as :</li>
                    <li><a href="javascript:void(0)" id="printPdfBtn" class="pdf_icon"></a></li>
                </ul>
            </span>

        </div>
    </form>
</div>

