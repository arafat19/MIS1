        <div class="form-group">
            <label class="col-md-2 control-label label-optional" for="beneficiaryAddress">Beneficiary Address:</label>

            <div class="col-md-3">
                <input class="k-textbox" id="beneficiaryAddress" name="beneficiaryAddress" tabindex="16"
                       placeholder="Beneficiary Address"/>
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-2 control-label label-optional" for="beneficiaryPhone">Beneficiary Phone:</label>

