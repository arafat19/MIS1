div.basic {
    margin: 0;
    padding: 0;
    font-family: "lucida grande", tahoma, verdana, arial, sans-serif;
    font-size: 13px;
}

.basic > a,
.basic > a.selected,
.basic > a.active,
.basic > a:hover {
    color: black;
    text-decoration: none;
    outline: none;
    border-top: 1px solid #FFF;
    border-bottom: 1px solid #999;
    cursor: pointer;
    display: block;
    padding: 6px;
}

.basic > a {
    background: #FFF url("../../images/accordion/tab0.gif");
}

.basic > a.selected,
.basic > a.active,
.basic > a:hover {
    background-image: url("../../images/accordion/tab2.gif");
}

.basic > a.active {
    color: #00C;
}

i.tap-pre {
    background-image: url("../../images/accordion/menu_icon_tab.png");
    background-repeat: no-repeat;
    display: inline-block;
    height: 22px;
    width: 22px;
    position: absolute;
}

