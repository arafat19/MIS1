package com.athena.mis.document.entity

class DocSubCategory {

    long id                   //Primary Key (Auto Generated By it's own sequence)
    int version
    long categoryId           //DocCategory.id
    String name               //Name of the Sub Category
    String description        //Description of the Sub Category
    String urlInName          //Sub Category url parameter name
    String url                //Sub Category URL
    boolean isActive          //Active/Inactive Sub Category
    long companyId            //Company.id
    Date createdOn            //Created Datetime
    long createdBy            //AppUser.id
    Date updatedOn            //Updated datetime
    long updatedBy            //AppUser.id


    static mapping = {
        id generator: 'sequence', params: [sequence: 'doc_sub_category_id_seq']
        categoryId index: 'doc_sub_category_category_id_idx'
        companyId index: 'doc_sub_category_company_id_idx'
        createdBy index: 'doc_sub_category_created_by_idx'
        updatedBy index: 'doc_sub_category_updated_by_idx'
    }
    static constraints = {
        name(nullable: false)
        categoryId(nullable: false)
        description(nullable: true)
        urlInName(nullable: false)
        url(nullable: false)
        isActive(nullable: false)
        companyId(nullable: false)
        createdOn(nullable: false)
        createdBy(nullable: false)
        updatedOn(nullable: true)
        updatedBy(nullable: false)
        name unique: 'categoryId'       //Sub Category name unique against categoryId

    }
}
