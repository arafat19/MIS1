<table class="table table-condensed table-bordered" style="margin:0">
    <tbody>
    <tr>
        <td class="active">Name:</td>
        <td>${customerInfoMap?.name}</td>
        <td class="active">Address Verified:</td>
        <td>${customerInfoMap?.addressVerifiedStatus ? 'Yes' : ''}</td>
    </tr>
    <tr>
        <td class="active">Nationality:</td>
        <td>${customerInfoMap?.country ? customerInfoMap.country : ''}</td>
        <td class="active">Phone:</td>
        <td>${customerInfoMap?.phone ? customerInfoMap.phone : ''}</td>
    </tr>
    <tr>
        <td class="active">Date Of Birth:</td>
        <td>${customerInfoMap?.dateOfBirth ? customerInfoMap.dateOfBirth : ''}</td>
        <td class="active">Email:</td>
        <td>${customerInfoMap?.email ? customerInfoMap.email : ''}</td>
    </tr>
    <tr>
        <td class="active">Post Code:</td>
        <td>${customerInfoMap?.postCode ? customerInfoMap.postCode : ''}</td>
        <td class="active">Photo ID Type:</td>
        <td>${customerInfoMap?.photoIdType ? customerInfoMap.photoIdType : ''}</td>
    </tr>
    <tr>
        <td class="active">Source of Fund:</td>
        <td>${customerInfoMap?.sourceOfFund ? customerInfoMap.sourceOfFund : ''}</td>
        <td class="active">Photo ID Expiry Date:</td>
        <td>${customerInfoMap?.photoIdExpiryDate ? customerInfoMap.photoIdExpiryDate : ''}</td>
    </tr>
    <tr>
        <td class="active">Address:</td>
        <td>${customerInfoMap?.address ? customerInfoMap.address : ''}</td>
        <td class="active">Photo ID No:</td>
        <td>${customerInfoMap?.photoIdNo ? customerInfoMap.photoIdNo : ''}</td>
    </tr>
    </tbody>
</table>
