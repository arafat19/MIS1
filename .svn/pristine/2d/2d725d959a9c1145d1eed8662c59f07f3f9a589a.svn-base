<div id="application_top_panel" class="panel panel-primary">
    <div class="panel-heading">
        <div class="panel-title">
            Show Outstanding Invitations
        </div>
    </div>

    <form id='outstandingInvitations' name='outstandingInvitations' class="form-horizontal form-widgets" role="form"
          method="post">
        <div class="panel-body">
            <div class="form-group">
                <div class="form-group col-md-4">
                    <label class="col-md-4 control-label label-required" for="fromDate">From Date:</label>

                    <div class="col-md-7">
                        <app:dateControl
                                name="fromDate"
                                tabindex="1">
                        </app:dateControl>
                    </div>
                </div>

                <div class="form-group col-md-4">
                    <label class="col-md-4 control-label label-required" for="toDate">To Date:</label>

                    <div class="col-md-7">
                        <app:dateControl
                                name="toDate"
                                diffWithCurrent="30"
                                tabindex="2">
                        </app:dateControl>
                    </div>
                </div>

                <div class="form-group col-md-4">
                    <label class="col-md-7 control-label label-optional"
                           for="acceptedInvitation">Accepted:</label>

                    <div class="col-md-3">
                        <input type="checkbox" id="acceptedInvitation" name="acceptedInvitation" tabindex="3"/>
                    </div>
                </div>
            </div>
        </div>

        <div class="panel-footer">
            <button name="create" id="create" type="submit" data-role="button"
                    class="k-button k-button-icontext"
                    role="button" tabindex="3"
                    aria-disabled="false"><span class="k-icon k-i-plus"></span>Search
            </button>
        </div>
    </form>
</div>


