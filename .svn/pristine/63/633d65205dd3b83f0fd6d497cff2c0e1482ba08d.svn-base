<div id="pinBranchInfo" style="display:none">
	<div class="form-group">
		<label class="col-md-6 control-label label-required" for="pinBankName">Bank:</label>
		<div class="col-md-6">
			<input type="text" class="k-textbox" name="pinBankName" id="pinBankName"
			       tabindex="15"/>
		</div>
	</div>

	<div class="form-group">
		<label class="col-md-6 control-label label-required" for="pinBankBranchName">Bank Branch:</label>
		<div class="col-md-6">
			<input type="text" class="k-textbox" name="pinBankBranchName" id="pinBankBranchName"
			       tabindex="15"/>
		</div>
	</div>

	<div class="form-group">
		<label class="col-md-6 control-label label-required" for="pinDistrictName">District:</label>
		<div class="col-md-6">
			<input type="text" class="k-textbox" name="pinDistrictName" id="pinDistrictName"
			       tabindex="15"/>
		</div>
	</div>
</div>


<div id="pinInfo" style="display:none">
	<div class="form-group">
		<label class="col-md-5 control-label" for="lblPinNo">PIN No:</label>
		<span id="lblPinNo" class="col-md-5">(Auto Generated)</span>
	</div>

	<div class="form-group">
		<label class="col-md-5 control-label label-required" for="identityType">Identity Type:</label>
		<div class="col-md-5">
			<input type="text" class="k-textbox" name="identityType" id="identityType" validationMessage=" "
			       tabindex="15"/>
		</div>
		<div class="col-md-2 pull-left">
			<span class="k-invalid-msg" data-for="identityType"></span>
		</div>
	</div>

	<div class="form-group">
		<label class="col-md-5 control-label" for="identityNo">Identity No:</label>
		<div class="col-md-5">
			<input type="text" class="k-textbox" name="identityNo" id="identityNo" validationMessage=" "
			       tabindex="15"/>
		</div>
		<div class="col-md-2 pull-left">
			<span class="k-invalid-msg" data-for="identityNo"></span>
		</div>
	</div>
</div>