<g:render template='/arms/report/beneficiary/scriptBeneficiaryDetails'/>

<div class="col-md-6" id="divBeneficiaryInfoGrid" style="padding-left: 0; padding-right: 5px">
    <table id="flex" style="display:none"></table>
</div>

<div class="col-md-6" style="padding-left: 5px; padding-right: 0">
    <div class="table-responsive" id="beneficiaryDetailsContainer">
        <rms:taskDetails property_name="${model?.property_name}" property_value="${model?.property_value}"
                         id="taskDetails" from_date="${model?.from_date}" to_date="${model?.to_date}"
                         url="${createLink(controller: 'rmsTask', action: 'reloadTaskDetailsTagLib')}">
        </rms:taskDetails>
    </div>
</div>


