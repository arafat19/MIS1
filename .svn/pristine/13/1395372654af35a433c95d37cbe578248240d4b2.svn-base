<div class="form-group">
    <div class="col-md-8">
        <div class="form-group">
            <table id="flex1" style="display:none"></table>
        </div>
    </div>

    <div class="col-md-4">
        <div class="panel panel-primary" id="containerSearchByModule">
            <div class="panel-heading">
                <div class="panel-title">
                    Search Bug
                </div>
            </div>

            <form id="taskForm" name="taskForm" class="form-horizontal form-widgets" role="form">
                <div class="panel-body">
                    <div class="form-group">

                        <div class="col-md-9">
                            <select tabindex="1"
                                    id="moduleId"
                                    name="moduleId">
                            </select>
                        </div>

                        <div class="col-md-3">
                            <button id="search" name="search" type="submit" data-role="button"
                                    class="k-button k-button-icontext"
                                    role="button" tabindex="2"
                                    aria-disabled="false"><span class="k-icon k-i-search"></span>
                            </button>
                        </div>
                    </div>

                    <div id="containerBugs">
                        <pt:bugList id="lstBugs" module_id="" url="/ptBug/bugListForModule">
                        </pt:bugList>
                    </div>

                </div>
            </form>
        </div>
    </div>
</div>
